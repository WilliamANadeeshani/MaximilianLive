<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>web.home.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountDetails</servlet-name>
        <servlet-class>web.home.speaker.AccountDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventDetails</servlet-name>
        <servlet-class>web.home.speaker.EventDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventDetailsView</servlet-name>
        <servlet-class>web.home.speaker.EventDetailsView</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountDetails</servlet-name>
        <url-pattern>/AccountDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventDetails</servlet-name>
        <url-pattern>/EventDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventDetailsView</servlet-name>
        <url-pattern>/EventDetailsView</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
